{% extends 'base.html.twig' %}

{% block title %}Tech_blog!{% endblock %}

{% block body %}
<h1>Tech Blog</h1>
<p>Latest Articles about Tech by Elon Musk</p>
<h2>Our Latest Posts</h2>
{% if posts %}
    <ul>
        {% for post in posts %}
            <li>{{ post.title }}</li>
        {% endfor %}
    </ul>
{% endif %}
{% endblock %}

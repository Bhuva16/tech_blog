{% extends 'base.html.twig' %}

{% block title %}Tech_blog!{% endblock %}

{% block body %}
<h1>Tech Blog</h1>
<p>Latest Articles about Tech by Elon Musk</p>
<h2>Our Latest Posts</h2>
{% if posts %}
    <article>
        {% for post in posts %}
            <h3>{{ post.title }}</h3>
        {% endfor %}
    </article>
{% endif %}
{% endblock %}
